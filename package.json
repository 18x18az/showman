{
  "name": "showman",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "chromatic": "chromatic --exit-zero-on-changes",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "ts-standard",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "env NODE_ENV=test storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook && npx http-server storybook-static --port 6006\" \"wait-on tcp:6006 && yarn test-storybook\""
  },
  "dependencies": {
    "@radix-ui/colors": "^3.0.0-rc.4",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "next": "13.4.19",
    "next-themes": "^0.2.1",
    "postcss": "8.4.29",
    "radix-colors-for-tailwind": "^2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-radix": "^2.8.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "7.4.1",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "7.4.1",
    "@storybook/jest": "^0.2.2",
    "@storybook/nextjs": "7.4.1",
    "@storybook/react": "7.4.1",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/react": "^14.0.0",
    "chromatic": "^7.1.0",
    "concurrently": "^8.2.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "storybook": "7.4.1",
    "ts-standard": "^12.0.2",
    "wait-on": "^7.0.1"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
